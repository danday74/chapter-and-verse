{
  "name": "node-template-18",
  "version": "1.0.3",
  "description": "Template for creating an npm package in 2018",
  "author": "danday74",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/istanbul cover -x \"**/*.spec.js\" ./node_modules/mocha/bin/_mocha -- js",
    "enforcer": "./node_modules/.bin/istanbul check-coverage --statement 100 --branch 100 --function 100 --line 100",
    "build": "npm-run-all test enforcer",
    "prepush": "npm test"
  },
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "2.11.16",
    "eslint": "^4.17.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "npm-run-all": "^4.1.2"
  },
  "comment": "coveralls@2.11.16"
}
